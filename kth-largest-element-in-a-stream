class KthLargest {
        private int k;
        PriorityQueue<Integer> pq=new PriorityQueue<>();
    public KthLargest(int k, int[] nums) {
      
         this.k = k;
   
    for (int i : nums) {
        pq.offer(i);
        if (pq.size() > k) pq.poll(); 
    }


        
    }
    
    public int add(int val) {
          pq.offer(val);
    if (pq.size() > k) pq.poll(); 
    return pq.peek();
        
    }
}
